{% extends 'base.html.twig' %}

{% block body %}
    <div id = wrapper>
        {% for one in product %}
            <option>{{ one }}</option>
        {% endfor %}
    </div>
{% endblock %}

{% block stylesheets %}
    <style>
        body { background: #F5F5F5; font: 18px/1.5 sans-serif; }
        #wrapper { background: #FFF; margin: 1em auto; max-width: 800px; width: 95%; }
        h { line-height: 1.2; margin: 0 0 .5em; font-size: 36px; }
    </style>
{% endblock %}